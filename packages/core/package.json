{
  "name": "@maiar-ai/core",
  "version": "0.25.0-alpha.2",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "prompts"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dist/logger": {
      "types": "./dist/logger.d.ts",
      "import": "./dist/logger.mjs",
      "require": "./dist/logger.js"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "import": "./dist/logger.mjs",
      "require": "./dist/logger.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "description": "Maiar: A Composable, Plugin-Based AI Agent Framework",
  "keywords": [
    "ai",
    "agent",
    "framework",
    "plugin",
    "core"
  ],
  "author": "UraniumCorporation <contact@maiar.dev> (https://maiar.dev)",
  "license": "MIT",
  "homepage": "https://maiar.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UraniumCorporation/maiar-ai.git"
  },
  "bugs": {
    "url": "https://github.com/UraniumCorporation/maiar-ai/issues"
  },
  "scripts": {
    "prepublishOnly": "pnpm run build",
    "dev": "tsup --config ./tsup.config.ts --watch",
    "typecheck": "tsc --project tsconfig.json --noEmit false",
    "build": "tsup --config ./tsup.config.ts",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.21.2",
    "fast-glob": "3.3.3",
    "liquidjs": "10.21.1",
    "openai": "4.82.0",
    "rxjs": "7.8.1",
    "winston": "3.17.0",
    "winston-transport": "4.9.0",
    "ws": "8.18.1",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "5.0.0",
    "@types/node": "22.13.1",
    "@types/ws": "8.18.1",
    "tsup": "8.3.6",
    "typescript": "5.7.3"
  }
}
