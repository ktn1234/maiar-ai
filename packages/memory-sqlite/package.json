  "homepage": "https://maiar.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UraniumCorporation/maiar-ai.git"
  },
  "bugs": {
    "url": "https://github.com/UraniumCorporation/maiar-ai/issues"
  },
  "scripts": {
    "dev": "tsup --config ../../tsup.config.base.ts --watch",
